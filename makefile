#sudo apt-get install Ubuntu




#Colores
rojo: 
	@tput bold;
	@tput setaf 1;

verde:
	@tput bold;
	@tput setaf 2;

magenta:
	@tput bold;
	@tput setaf 5;

negro:
	@tput bold;
	@tput setaf 5;

amarillo:
	@tput bold;
	@tput setaf 3;

transparente:
	@tput setaf 7;

restablecer:
	@tput sgr0;